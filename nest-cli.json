{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/library-backend-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/library-backend-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/library-backend-app",
  "projects": {
    "authors": {
      "type": "application",
      "root": "apps/authors",
      "entryFile": "main",
      "sourceRoot": "apps/authors/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authors/tsconfig.app.json"
      }
    },
    "books": {
      "type": "application",
      "root": "apps/books",
      "entryFile": "main",
      "sourceRoot": "apps/books/src",
      "compilerOptions": {
        "tsConfigPath": "apps/books/tsconfig.app.json"
      }
    },
    "categories": {
      "type": "application",
      "root": "apps/categories",
      "entryFile": "main",
      "sourceRoot": "apps/categories/src",
      "compilerOptions": {
        "tsConfigPath": "apps/categories/tsconfig.app.json"
      }
    },
    "library-backend-app": {
      "type": "application",
      "root": "apps/library-backend-app",
      "entryFile": "main",
      "sourceRoot": "apps/library-backend-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/library-backend-app/tsconfig.app.json"
      }
    }
  }
}